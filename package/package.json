{
  "name": "@gfazioli/mantine-mask",
  "version": "0.5.4",
  "description": "A Mantine extension component that applies a cursorâ€‘follow or static spotlight using CSS masks, revealing content inside the spotlight and softly fading the outside.",
  "homepage": "https://gfazioli.github.io/mantine-mask/",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Giovambattista Fazioli <giovambattista.fazioli@gmail.com>",
  "keywords": [
    "extension",
    "mask",
    "spotlight",
    "overlay",
    "mantine",
    "react",
    "react-component",
    "typescript"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
